<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/ns/no_namespace_schema_location/Ressources/FormatFichier.sharedparse" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns2="http://www.tibco.com/schemas/Lab2/schema/Schema.xsd" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:ns3="http://www.tibco.com/namespaces/tnt/plugins/file">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/ns/no_namespace_schema_location/Ressources/FormatFichier.sharedparse" schemaLocation="/Ressources/FormatFichier.sharedparse"/>
    <pd:name>ProcessDefinitions/Ecriture csv.process</pd:name>
    <pd:startName>JMS Queue Receiver</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="JMS Queue Receiver">
        <pd:type>com.tibco.plugin.jms.JMSQueueEventSource</pd:type>
        <pd:resourceType>ae.activities.JMSQueueEventSource</pd:resourceType>
        <pd:x>105</pd:x>
        <pd:y>102</pd:y>
        <config>
            <PermittedMessageType>XML Text</PermittedMessageType>
            <SessionAttributes>
                <transacted>false</transacted>
                <acknowledgeMode>2</acknowledgeMode>
                <maxSessions/>
                <destination>transport</destination>
            </SessionAttributes>
            <ConfigurableHeaders>
                <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                <JMSExpiration>0</JMSExpiration>
                <JMSPriority>4</JMSPriority>
            </ConfigurableHeaders>
            <ConnectionReference>/Connections/ConnexionJMS.sharedjmscon</ConnectionReference>
            <OutDataxsdString ref="pfx:Rows"/>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>1106</pd:endX>
    <pd:endY>113</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1618483704868</pd:targetNamespace>
    <pd:activity name="Log Initiale">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>246</pd:x>
        <pd:y>115</pd:y>
        <config>
            <processName>/ProcessDefinitions/Logging.process</processName>
        </config>
        <pd:inputBindings>
            <ns2:log_info>
                <ns2:idFonc>
                    <xsl:value-of select="concat(&quot;????&quot;, tib:timestamp())"/>
                </ns2:idFonc>
                <ns2:idTech>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProcessId"/>
                </ns2:idTech>
                <ns2:status>
                    <xsl:value-of select="&quot;N/A&quot;"/>
                </ns2:status>
                <ns2:logMessage>
                    <xsl:value-of select="&quot;N/A&quot;"/>
                </ns2:logMessage>
            </ns2:log_info>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Confirm">
        <pd:type>com.tibco.pe.core.ConfirmActivity</pd:type>
        <pd:resourceType>activity.confirm</pd:resourceType>
        <pd:x>899</pd:x>
        <pd:y>104</pd:y>
        <config>
            <ConfirmEvent>JMS Queue Receiver</ConfirmEvent>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="JDBC Update">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>723</pd:x>
        <pd:y>130</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/Connections/JDBC Connection.sharedjdbc</jdbcSharedConfig>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Create File csv">
        <pd:type>com.tibco.plugin.file.FileCreateActivity</pd:type>
        <pd:resourceType>ae.activities.FileCreateActivity</pd:resourceType>
        <pd:x>405</pd:x>
        <pd:y>138</pd:y>
        <config/>
        <pd:inputBindings>
            <ns3:CreateActivityInputClass>
                <fileName>
                    <xsl:value-of select="concat($JMS-Queue-Receiver/ns1:ActivityOutput/Body/Rows/Client/nom,&quot;_&quot;, $JMS-Queue-Receiver/ns1:ActivityOutput/Body/Rows/Client/prenom, &quot;_&quot;, tib:format-date(&quot;ddMMyyyy&quot;,$JMS-Queue-Receiver/ns1:ActivityOutput/Body/Rows/Client/date_ajout), &quot;_&quot;,  $JMS-Queue-Receiver/ns1:ActivityOutput/Body/Rows/Client/id_client)"/>
                </fileName>
            </ns3:CreateActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Write File csv">
        <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
        <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
        <pd:x>559</pd:x>
        <pd:y>82</pd:y>
        <config>
            <encoding>text</encoding>
            <compressFile>None</compressFile>
        </config>
        <pd:inputBindings>
            <ns3:WriteActivityInputTextClass>
                <fileName>
                    <xsl:value-of select="$Create-File-csv/ns3:CreateActivityOutputClass/fileInfo/fullName"/>
                </fileName>
                <textContent>
                    <xsl:value-of select="concat($JMS-Queue-Receiver/ns1:ActivityOutput/Body/Rows/Client/id_client,&quot;,&quot;, $JMS-Queue-Receiver/ns1:ActivityOutput/Body/Rows/Client/nom,&quot;,&quot;,$JMS-Queue-Receiver/ns1:ActivityOutput/Body/Rows/Client/prenom,&quot;,&quot;,$JMS-Queue-Receiver/ns1:ActivityOutput/Body/Rows/Client/adresse,&quot;,&quot;,$JMS-Queue-Receiver/ns1:ActivityOutput/Body/Rows/Client/profession,&quot;,&quot;,$JMS-Queue-Receiver/ns1:ActivityOutput/Body/Rows/Client/date_ajout,&quot;,&quot;,$JMS-Queue-Receiver/ns1:ActivityOutput/Body/Rows/Client/date_update,&quot;,&quot;,$JMS-Queue-Receiver/ns1:ActivityOutput/Body/Rows/Client/flag)"/>
                </textContent>
            </ns3:WriteActivityInputTextClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>JMS Queue Receiver</pd:from>
        <pd:to>Log Initiale</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Confirm</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>JDBC Update</pd:from>
        <pd:to>Confirm</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log Initiale</pd:from>
        <pd:to>Create File csv</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Create File csv</pd:from>
        <pd:to>Write File csv</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Write File csv</pd:from>
        <pd:to>JDBC Update</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>