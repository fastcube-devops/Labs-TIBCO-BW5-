<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/ns/no_namespace_schema_location/Ressources/FormatFichier.sharedparse" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/schemas/Lab2/schema/Schema.xsd" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns1="http://www.tibco.com/pe/EngineTypes">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/ns/no_namespace_schema_location/Ressources/FormatFichier.sharedparse" schemaLocation="/Ressources/FormatFichier.sharedparse"/>
    <pd:name>ProcessDefinitions/Lab1.process</pd:name>
    <pd:startName>Timer</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="Timer">
        <pd:type>com.tibco.plugin.timer.TimerEventSource</pd:type>
        <pd:resourceType>ae.activities.timer</pd:resourceType>
        <pd:x>67</pd:x>
        <pd:y>72</pd:y>
        <config>
            <StartTime>1618483505000</StartTime>
            <Frequency>false</Frequency>
            <TimeInterval>20</TimeInterval>
            <FrequencyIndex>Second</FrequencyIndex>
        </config>
        <pd:inputBindings/>
